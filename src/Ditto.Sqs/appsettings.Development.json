{
  "Settings": {
    "TimeToLive": "00:01:00",
    "SourceEventStoreConnectionString": "ConnectTo=tcp://admin:changeit@localhost:1113;",
    "Subscriptions": [
      {
        "StreamName": "$ce-customer",
        "GroupName": "ditto-sqs-customer"
      }
    ],
    "SkipVersionCheck": false,
    "PersistentSubscriptionBufferSize": 100
  },
  "Sqs": {
    "QueueUrl": "https://sqs.eu-west-1.amazonaws.com/000000000000/ditto",
    "QueueName": "ditto"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "restrictedToMinimumLevel": "Debug"
        }
      }
    ]
  },
  "AWS": {
    "ServiceURL": "http://localhost:4566"
  }
}